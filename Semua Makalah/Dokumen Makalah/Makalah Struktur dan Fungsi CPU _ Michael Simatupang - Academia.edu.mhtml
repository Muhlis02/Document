@charset "utf-8";

.author { display: grid; grid-auto-flow: column; gap: 6px; }

.author:not(:last-of-type) { margin-right: 6px; }

.author img { border-radius: 50%; height: 20px; width: 20px; }

.authors { display: flex; margin-bottom: 12px; }

.button-container { height: 55px; }

.loading-state-container { background: rgb(255, 255, 255); display: grid; height: calc(100vh - var(--nav-mobile-height)); grid-template-columns: 1fr; overflow: hidden; width: 100vw; }

.loading-state-container ~ * { display: none !important; }

.metadata { height: 24px; }

.page-content { display: grid; grid-template-rows: max-content 1fr; height: 100%; place-items: start center; width: 100%; }

.research-interests { height: 24px; }

.sidebar-content { display: none; height: 100%; box-shadow: rgba(0, 0, 0, 0.25) 0px 2px 3px; max-width: 460px; }

.tabs-section { background: rgb(243, 244, 250); border-top: 4px solid rgb(230, 237, 250); height: 100%; width: 100%; }

.top-card { padding-bottom: 130px; padding-left: 20px; padding-right: 15px; }

.work-abstract { -webkit-line-clamp: 3; -webkit-box-orient: vertical; display: -webkit-box; max-width: 760px; margin-bottom: 18px; overflow: hidden; }

.work-title { color: rgb(51, 51, 51); font-family: Georgia, Times, "Times New Roman", serif; font-size: 24px; line-height: 1.4em; margin-bottom: 16px; margin-top: 0px; }

@media (min-width: 993px) {
  .loading-state-container { grid-template-columns: 1fr 460px; }
  .sidebar-content { display: grid; }
}

@media (min-width: 545px) {
  .button-container { height: 70px; }
}

@media (min-width: 768px) {
  .loading-state-container { height: calc(100vh - var(--nav-desktop-height)); }
  .top-card { padding: 40px 40px 130px; }
  h1.work-title { font-size: 34px; }
}

@media (min-width: 1500px) {
  .top-card { width: 1039px; }
}
