@charset "utf-8";

.iosgettheappmodal { background-color: white; }

.iosgettheappmodal html { font-family: sans-serif; text-size-adjust: 100%; }

.iosgettheappmodal body { margin: 0px; }

.iosgettheappmodal article, .iosgettheappmodal aside, .iosgettheappmodal details, .iosgettheappmodal figcaption, .iosgettheappmodal figure, .iosgettheappmodal footer, .iosgettheappmodal header, .iosgettheappmodal hgroup, .iosgettheappmodal main, .iosgettheappmodal menu, .iosgettheappmodal nav, .iosgettheappmodal section, .iosgettheappmodal summary { display: block; }

.iosgettheappmodal audio, .iosgettheappmodal canvas, .iosgettheappmodal progress, .iosgettheappmodal video { display: inline-block; vertical-align: baseline; }

.iosgettheappmodal audio:not([controls]) { display: none; height: 0px; }

.iosgettheappmodal [hidden], .iosgettheappmodal template { display: none; }

.iosgettheappmodal a { background-color: transparent; }

.iosgettheappmodal a:active, .iosgettheappmodal a:hover { outline: 0px; }

.iosgettheappmodal abbr[title] { border-bottom: 1px dotted; }

.iosgettheappmodal b, .iosgettheappmodal strong { font-weight: bold; }

.iosgettheappmodal dfn { font-style: italic; }

.iosgettheappmodal h1 { font-size: 2em; margin: 0.67em 0px; }

.iosgettheappmodal mark { background: rgb(255, 255, 0); color: rgb(0, 0, 0); }

.iosgettheappmodal small { font-size: 80%; }

.iosgettheappmodal sub, .iosgettheappmodal sup { font-size: 75%; line-height: 0; position: relative; vertical-align: baseline; }

.iosgettheappmodal sup { top: -0.5em; }

.iosgettheappmodal sub { bottom: -0.25em; }

.iosgettheappmodal img { border: 0px; }

.iosgettheappmodal svg:not(:root) { overflow: hidden; }

.iosgettheappmodal figure { margin: 1em 40px; }

.iosgettheappmodal hr { box-sizing: content-box; height: 0px; }

.iosgettheappmodal pre { overflow: auto; }

.iosgettheappmodal code, .iosgettheappmodal kbd, .iosgettheappmodal pre, .iosgettheappmodal samp { font-family: monospace, monospace; font-size: 1em; }

.iosgettheappmodal button, .iosgettheappmodal input, .iosgettheappmodal optgroup, .iosgettheappmodal select, .iosgettheappmodal textarea { color: inherit; font: inherit; margin: 0px; }

.iosgettheappmodal button { overflow: visible; }

.iosgettheappmodal button, .iosgettheappmodal select { text-transform: none; }

.iosgettheappmodal button, .iosgettheappmodal html input[type="button"], .iosgettheappmodal input[type="reset"] { appearance: button; cursor: pointer; }

.iosgettheappmodal button[disabled], .iosgettheappmodal html input[disabled] { cursor: default; }

.iosgettheappmodal input { line-height: normal; }

.iosgettheappmodal input[type="checkbox"], .iosgettheappmodal input[type="radio"] { box-sizing: border-box; padding: 0px; }

.iosgettheappmodal input[type="number"]::-webkit-inner-spin-button, .iosgettheappmodal input[type="number"]::-webkit-outer-spin-button { height: auto; }

.iosgettheappmodal input[type="search"] { appearance: none; }

.iosgettheappmodal input[type="search"]::-webkit-search-cancel-button, .iosgettheappmodal input[type="search"]::-webkit-search-decoration { appearance: none; }

.iosgettheappmodal fieldset { border: 1px solid rgb(192, 192, 192); margin: 0px 2px; padding: 0.35em 0.625em 0.75em; }

.iosgettheappmodal legend { border: 0px; padding: 0px; }

.iosgettheappmodal textarea { overflow: auto; }

.iosgettheappmodal optgroup { font-weight: bold; }

.iosgettheappmodal table { border-collapse: collapse; border-spacing: 0px; }

.iosgettheappmodal td, .iosgettheappmodal th { padding: 0px; }

@font-face { }

.iosgettheappmodal [class^="w-icon-"], .iosgettheappmodal [class*=" w-icon-"] { speak: none; font-style: normal; font-weight: normal; font-variant: normal; text-transform: none; line-height: 1; -webkit-font-smoothing: antialiased; font-family: webflow-icons !important; }

.iosgettheappmodal .w-icon-slider-right::before { content: ""; }

.iosgettheappmodal .w-icon-slider-left::before { content: ""; }

.iosgettheappmodal .w-icon-nav-menu::before { content: ""; }

.iosgettheappmodal .w-icon-arrow-down::before, .iosgettheappmodal .w-icon-dropdown-toggle::before { content: ""; }

.iosgettheappmodal .w-icon-file-upload-remove::before { content: ""; }

.iosgettheappmodal .w-icon-file-upload-icon::before { content: ""; }

.iosgettheappmodal * { box-sizing: border-box; }

.iosgettheappmodal html { height: 100%; }

.iosgettheappmodal body { margin: 0px; min-height: 100%; background-color: rgb(255, 255, 255); font-family: Arial, sans-serif; font-size: 14px; line-height: 20px; color: rgb(51, 51, 51); }

.iosgettheappmodal img { max-width: 100%; vertical-align: middle; display: inline-block; }

